{
  "name": "ceros-ski",
  "version": "1.0.0",
  "description": "Ceros full stack developer code challenge",
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "test": "jest --coverage=true",
    "test:debug": "jest --coverage=true --watch",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceros/ceros-ski.git"
  },
  "author": "Dan Wash",
  "license": "",
  "homepage": "https://github.com/ceros/ceros-ski#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@types/jest": "^26.0.10",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1"
  }
}
