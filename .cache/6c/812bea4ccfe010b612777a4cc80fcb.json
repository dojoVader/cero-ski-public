},"generated":{"line":79,"column":1}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":0},"generated":{"line":79,"column":3}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":0}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":7}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":8}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":20}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":23}},{"source":"engine/AssetManager.ts","name":null,"original":{"line":27,"column":13},"generated":{"line":81,"column":35}}],"sources":{"engine/AssetManager.ts":"// Handles loading the assets for the game that is needed\r\n/**\r\n * @todo Implement sound loader\r\n */\r\nimport { LoDashStatic } from \"lodash\";\r\n\r\ndeclare var _: LoDashStatic\r\ndeclare var $: JQueryStatic;\r\nconst assets = {\r\n    'skierCrash': 'img/skier_crash.png',\r\n    'skierLeft': 'img/skier_left.png',\r\n    'skierLeftDown': 'img/skier_left_down.png',\r\n    'skierDown': 'img/skier_down.png',\r\n    'skierRightDown': 'img/skier_right_down.png',\r\n    'skierRight': 'img/skier_right.png',\r\n    'tree': 'img/tree_1.png',\r\n    'treeCluster': 'img/tree_cluster.png',\r\n    'rock1': 'img/rock_1.png',\r\n    'rock2': 'img/rock_2.png',\r\n    'jumpRamp': 'img/jump_ramp.png'\r\n};\r\n\r\n\r\n/**\r\n * @todo Implement functionality to allow loading assets via JSON\r\n */\r\nexport class AssetManager {\r\n\r\n    private _assetResourceList: object; // This stores the list of the assets to be downloaded\r\n\r\n    private _trackingManager = {\r\n        failed: 0,\r\n        passed: 0\r\n    }\r\n    constructor() {\r\n        this._assetResourceList = assets;\r\n    }\r\n\r\n    download(): JQueryDeferred<any> {\r\n        let deferredPromises = [];\r\n        _.each(assets, (asset, assetName) => {\r\n         \r\n            let resourceImage = new Image();\r\n            let deferred = $.Deferred();\r\n\r\n            resourceImage.onload = () => {\r\n                resourceImage.width /= 2;\r\n                resourceImage.height /= 2;\r\n                this.setAsset(assetName, resourceImage);\r\n                this._trackingManager.passed++;\r\n                deferred.resolve();\r\n            }\r\n\r\n            resourceImage.onerror = (e) => {\r\n                this._trackingManager.failed++;\r\n            }\r\n            //Set the resource to the source to trigger the events\r\n            resourceImage.src = asset;\r\n        });\r\n        return $.when.apply($, deferredPromises);\r\n    }\r\n\r\n    setAsset(name: string, resource: any) {\r\n        this._assetResourceList[name] = resource;\r\n    }\r\n    getAsset(name: string) {\r\n        if (!this._assetResourceList[name]) {\r\n            throw new Error(' Asset has not been defined in the resource list')''\r\n        }\r\n        return this._assetResourceList[name];\r\n    }\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"ff7057d1b1201c8ae44b3c6dd29ca1ae","cacheData":{"env":{}}}